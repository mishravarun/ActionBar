<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <!--You may know card -->
    <style name="carddemo_mayknow_main_inner_title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginLeft">@dimen/card_header_simple_title_margin_left</item>
    </style>

    <style name="carddemo_mayknow_main_inner_title.sub">
        <item name="android:textSize">1sp</item>
        <item name="android:textColor">@android:color/darker_gray</item>
    </style>

    <!-- Suggested community -->

    <style name="carddemo_suggested_header_text"></style>

    <style name="carddemo_suggested_text">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#D0D0D0</item>
    </style>

    <style name="carddemo_suggested_text_bold">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="carddemo_suggested_text_bold1">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>


</resources>
